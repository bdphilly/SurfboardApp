<div class="modal fade" id="new-renter-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel">Booking</h4>
      </div>
      <div class="modal-body">
        <br>
        Brand: <%= board.escape('brand') %><br>
        Model: <%= board.escape('model') %><br>
        Price: <%= board.escape('price') %>

          <br><br>

      <div class="input-daterange input-group col-lg-6" id="datepicker2">
        
        <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
        <label class="sr-only" for="show-start-date">Start Date</label>
        <input type="text" class="form-control" id="show-start-date" placeholder="Start Date" name="filters[start]"/>

        <span class="input-group-addon separator">to</span>

        <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
        <label class="sr-only" for="show-end-date">End Date</label>
        <input type="text" class="form-control" id="show-end-date" placeholder="End Date" name="filters[end]"/>
      </div>


      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
        <button type="button" id="renter-submit" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>